Model = load('GH_PKC');
%-------------------------------------------------------------------
S = Model.S;
%-------------------------------------------------------------------
[s,r] = size(S);
%-------------------------------------------------------------------
syms x  [1 s]  real
x = x';
Gamma_x = diag(x);
%-------------------------------------------------------------------
syms k [1 r] real
k = k';
%-------------------------------------------------------------------
q = 5;
syms K [1 q] real
K = K';
%-------------------------------------------------------------------
syms v [1 r] real
v = v';
Gamma_v = diag(v);
%-------------------------------------------------------------------
g = GH_Denominator (x,K);
Gamma_g = diag(g);
%-------------------------------------------------------------------
dx_dt = S*v;
%-------------------------------------------------------------------
grad_g = GH_Nabla_g (x,K);
%-------------------------------------------------------------------
   for i = 1:r
       N_i = grad_g(i,:);
       output(i) = dot(N_i, dx_dt);
   end
psi = simplify(output');
%-------------------------------------------------------------------
Delta = - min(S,0);
%-------------------------------------------------------------------
dv_dt = Gamma_v*inv(Gamma_g)*psi + Gamma_v*Delta'*inv(Gamma_x)*S*v
